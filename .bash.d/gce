# The next line updates PATH for the Google Cloud SDK.
source '/Users/adamtait/.google-cloud-sdk/path.bash.inc'

# The next line enables bash completion for gcloud.
source '/Users/adamtait/.google-cloud-sdk/completion.bash.inc'

# The next line updates PATH for the Google Cloud SDK.
export PATH=/Users/adamtait/google-cloud-sdk/bin:$PATH

GCLOUD_PROJECT_IDS=('white-might-266' 'ornate-grail-425')

function ssh-gc {
  if [ $1 == static ] ; then
    gcutil getproject --project=ornate-grail-425 --cache_flag_values
    gcutil ssh god-server-micro
  else
    gcutil getproject --project=white-might-266 --cache_flag_values
    gcutil ssh howsmyboyfriend-prod2
  fi
}
