
(package-initialize)

(setq custom-file "~/.dotfiles/configuration/emacs/emacs-custom.el")
(load custom-file)


;;------------------------------------------------
;; Locally-installed packages (non-ELPA)

(push "~/.dotfiles/configuration/emacs/d/restclient" load-path)
(push "~/.dotfiles/configuration/emacs/d/org-mode/contrib/lisp" load-path)
(push "~/.dotfiles/configuration/emacs/d/org-mode/lisp" load-path)
(push "~/.dotfiles/configuration/emacs/d/leuven-theme/lisp" load-path)
(push "~/.dotfiles/configuration/emacs/d/local/" load-path)


;;------------------------------------------------
;; Init

(load "init")
